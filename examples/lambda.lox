fun fibnacii() {
    var prev = 0, cur = 0

    return () -> {
        if (cur == 0) {
            cur = 1
            return 0
        }

        var res = cur
        cur += prev
        prev = res

        return res
    }
}

var next = fibnacii()
print next()
